<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
    <metadata>
        <id>UcdArchBoilerplate</id>
        <version>5.0.0</version>
        <authors>Scott Kirkland</authors>
        <owners>Scott Kirkland</owners>
        <licenseUrl>https://github.com/ucdavis/UCDArch/blob/master/LICENSE.txt</licenseUrl>
        <projectUrl>https://github.com/ucdavis/UCDArch</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Everything you need to start an ASPNET MVC project w/ UCDArch and the standard boilerplate styles</description>
        <summary>Adds twitter bootstrap styling, datatables custom styling, style/script bundles, and a sample layout page website</summary>
        <language>en-US</language>
        <dependencies>
            <dependency id="Twitter.Bootstrap" version="3.0.1.1" />
            <dependency id="FontAwesome" version="4.0.3.1" />
            <dependency id="UcdArch.Mvc" version="5.1.0.120" />
        </dependencies>
    </metadata>
    <files>
      <file src="SampleUCDArchApp\SampleUCDArchApp\Content\custom.css" target="Content/Content/custom.css" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\Content\images\debut_light.png" target="Content/Content/images/debut_light.png" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\Content\images\logo.png" target="Content/Content/images/logo.png" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\Content\DataTables-1.9.4\**" target="Content/Content/DataTables-1.9.4/" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\Scripts\DataTables-1.9.4\**" target="Content/Scripts/DataTables-1.9.4/" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\Scripts\datatables-bootstrap.js" target="Content/Scripts/datatables-bootstrap.js" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\App_Start\BundleBootstrapper.cs.pp" target="Content/App_Start/BundleBootstrapper.cs.pp" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\favicon.ico" target="Content/favicon.ico" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\favicon.ico" target="Content/faviconUcdArch.ico" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\robots.txt" target="Content/robots.txt" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\Views\Shared\_Layout.cshtml" target="Content/Views/Shared/_LayoutUcdArch.cshtml" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\Views\Shared\_Navigation.cshtml" target="Content/Views/Shared/_Navigation.cshtml" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\Views\Shared\_ServiceMessages.cshtml" target="Content/Views/Shared/_ServiceMessages.cshtml" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\Controllers\ApplicationController.cs.pp" target="Content/Controllers/ApplicationController.cs.pp" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\Controllers\SampleController.cs.pp" target="Content/Controllers/SampleController.cs.pp" />
      <file src="SampleUCDArchApp\SampleUCDArchApp\Views\Sample\*" target="Content/Views/Sample/" />
    </files>
</package>