<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
    <metadata>
        <id>UcdArchBoilerplate</id>
        <version>4.0.0</version>
        <authors>Scott Kirkland</authors>
        <owners>Scott Kirkland</owners>
        <licenseUrl>https://github.com/ucdavis/UCDArch/blob/master/LICENSE.txt</licenseUrl>
        <projectUrl>https://github.com/ucdavis/UCDArch</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Everything you need to start an ASPNET MVC project w/ UCDArch and the standard boilerplate styles</description>
        <summary>Adds twitter bootstrap styling, datatables custom styling, style/script bundles, and a sample layout page website</summary>
        <language>en-US</language>
        <dependencies>
            <dependency id="Twitter.Bootstrap" version="2.2.2" />
            <dependency id="UcdArch.Mvc" version="4.0.95" />
        </dependencies>
    </metadata>
    <files>
        <file src="..\..\SampleUCDArchApp\Content\bootstrap-custom.css" target="Content/Content/bootstrap-custom.css" />
        <file src="..\..\SampleUCDArchApp\Content\custom.css" target="Content/Content/custom.css" />
        <file src="..\..\SampleUCDArchApp\Content\DataTables-1.9.4\**" target="Content/Content/DataTables-1.9.4/" />
        <file src="..\..\SampleUCDArchApp\Scripts\DataTables-1.9.4\**" target="Content/Scripts/DataTables-1.9.4/" />
        <file src="..\..\SampleUCDArchApp\App_Start\BundleBootstrapper.cs.pp" target="Content/App_Start/BundleBootstrapper.cs.pp" />
        <file src="..\..\SampleUCDArchApp\favicon.ico" target="Content/favicon.ico" />
        <file src="..\..\SampleUCDArchApp\favicon.ico" target="Content/faviconUcdArch.ico" />
        <file src="..\..\SampleUCDArchApp\robots.txt" target="Content/robots.txt" />
        <file src="..\..\SampleUCDArchApp\Views\Shared\_Layout.cshtml" target="Content/Views/Shared/_LayoutUcdArch.cshtml" />
        <file src="..\..\SampleUCDArchApp\Views\Shared\_Navigation.cshtml" target="Content/Views/Shared/_Navigation.cshtml" />
        <file src="..\..\SampleUCDArchApp\Views\Shared\_ServiceMessages.cshtml" target="Content/Views/Shared/_ServiceMessages.cshtml" />
    </files>
</package>